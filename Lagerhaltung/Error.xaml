<Window x:Class="Lagerhaltung.Error"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lagerhaltung"
        mc:Ignorable="d"
        Title="Error" Height="200" Width="200"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Fehler" HorizontalAlignment="Center" Margin="0,45,0,0" VerticalAlignment="Top" FontSize="30"/>
        <Button Content="Okay" HorizontalAlignment="Center" Margin="0,122,0,0" VerticalAlignment="Top" Click="OnClickOkay"/>

    </Grid>
</Window>
